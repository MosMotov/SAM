int currentPixel = 0;
int cmd;


void setup() {
  // Open serial communications and wait for port to open:
  Serial.begin(115200);

  // set the data rate for the SoftwareSerial port
  mySerial.begin(9600);

  currentPixel = 0;
  pixels.begin();
  pixels.show();
}

void loop() { // run over and over
  if (mySerial.available()) {
    cmd = int(mySerial.read());
    Serial.println(cmd);
  }
  if (cmd == 97) {
    if ((unsigned long)(millis() - measurePreviousMillis) >= measure_interval) {
      measurePreviousMillis = millis();
      pattern(taking_measurement, measure_pixel, measure_frames);
    }
  }
  else if (cmd == 98) {
    if ((unsigned long)(millis() - alarmPreviousMillis) >= alarm_interval) {
      alarmPreviousMillis = millis();
      pattern(alarm, alarm_pixel, alarm_frames);
    }
  }
  else if (cmd == 99) {
    if ((unsigned long)(millis() - whPreviousMillis) >= wh_interval) {
      whPreviousMillis = millis();
      pattern(taking_wh, wh_pixel, wh_frames);
    }
  }

}

void pattern(byte data[], int &pattern_pixel, int frames) {
  for (int i = 0; i < NUMPIXELS; i++) {
    int pixelIndex = i % NUMPIXELS;
    int index = pattern_pixel * NUMPIXELS * 3 + pixelIndex * 3;

    //Note: We're using pgm_read_byte_near to read bytes out of the data array stored in PROGMEM. These functions are not required for all configurations
    pixels.setPixelColor(i, pgm_read_byte_near(data + index), pgm_read_byte_near(data + index + 1), pgm_read_byte_near(data + index + 2));
  }
  pixels.show();
  pattern_pixel ++;
  if (pattern_pixel >= frames) pattern_pixel = 0;
}

void rainbow() {
  for (uint16_t i = 0; i < pixels.numPixels(); i++) {
    pixels.setPixelColor(i, Wheel((i + rainbowCycles) & 255));
  }
  pixels.show();
  rainbowCycles++;
  if (rainbowCycles >= 256) rainbowCycles = 0;
}

// Slightly different, this makes the rainbow equally distributed throughout
void rainbowCycle() {
  uint16_t i;

  //for(j=0; j<256*5; j++) { // 5 cycles of all colors on wheel
  for (i = 0; i < pixels.numPixels(); i++) {
    pixels.setPixelColor(i, Wheel(((i * 256 / pixels.numPixels()) + rainbowCycleCycles) & 255));
  }
  pixels.show();

  rainbowCycleCycles++;
  if (rainbowCycleCycles >= 256 * 5) rainbowCycleCycles = 0;
}

// Input a value 0 to 255 to get a color value.
// The colours are a transition r - g - b - back to r.
uint32_t Wheel(byte WheelPos) {
  WheelPos = 255 - WheelPos;
  if (WheelPos < 85) {
    return pixels.Color(255 - WheelPos * 3, 0, WheelPos * 3);
  }
  if (WheelPos < 170) {
    WheelPos -= 85;
    return pixels.Color(0, WheelPos * 3, 255 - WheelPos * 3);
  }
  WheelPos -= 170;
  return pixels.Color(WheelPos * 3, 255 - WheelPos * 3, 0);
}


